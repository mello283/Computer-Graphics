<Window x:Class="MorrisonLab03.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MorrisonLab03"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Viewbox HorizontalAlignment="Left" Width="800">
        <StackPanel Height="100" Width="100">
            <StackPanel Orientation ="Horizontal" Height="100">
                <Button Content="btnStart"/>
                <TextBlock x:Name="tbLabel" Text="tblabe" TextWrapping="Wrap"/>
            </StackPanel>
            <Canvas x:Name="canvas1" Height="300px" Width="300px" Margin="5px" Background="#FFFEFEFE"/>
        </StackPanel>
    </Viewbox>
</Window>
